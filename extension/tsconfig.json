{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "resolveJsonModule": true,
    "types": ["chrome", "react"],
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["src", "manifest.json", "vite.config.*"]
}

